{
  "name": "UVAutomate",
  "description": "Client to automate UVA Netbadge Login",
  "version": "1.1",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js"
  },
  "host_permissions": ["https://*/*", "http://*/*"],
  "permissions": ["storage", "activeTab", "scripting", "tabs", "cookies"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/uva16.png",
      "48": "icons/uva48.png",
      "128": "icons/uva128.png"
    }
  },
  "icons": {
    "16": "icons/uva16.png",
    "48": "icons/uva48.png",
    "128": "icons/uva128.png"
  }
}
